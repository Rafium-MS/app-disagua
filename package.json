{
  "name": "app-disagua",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron/main.ts",
  "scripts": {
    "migrate:deploy": "prisma migrate deploy",
    "predev": "npm run migrate:deploy --silent && npm run generate --silent",
    "dev": "concurrently -k -n VITE,ELECTRON \"vite\" \"wait-on tcp:5173 && electron electron/dev-main.cjs\"",
    "server": "tsx src/server/index.ts",
    "migrate": "prisma migrate dev --skip-generate --skip-seed",
    "seed": "npm run migrate && prisma db seed",
    "db:setup": "npm run migrate && prisma db seed",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint \"{electron,src,tests}/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:renderer": "vite build",
    "build:main": "tsc -p tsconfig.build.json",
    "build": "npm run build:main && npm run build:renderer",
    "generate": "prisma generate",
    "import:stores": "tsx scripts/import-stores.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "archiver": "^6.0.2",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "zod": "^3.23.8",
    "class-variance-authority": "^0.7.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pdf-lib": "^1.17.1",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "tailwind-merge": "^2.5.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@testing-library/react": "^16.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "electron": "^31.3.1",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.4.41",
    "prisma": "^5.18.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vitest": "^2.1.1",
    "wait-on": "^8.0.2"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
