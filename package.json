{
  "name": "app-disagua",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "electron/main.ts",
  "scripts": {
    "dev": "concurrently -k -n VITE,ELECTRON \"vite\" \"wait-on tcp:5173 && electron -r ts-node/register/transpile-only -r tsconfig-paths/register electron/main.ts\"",
    "server": "ts-node --transpile-only src/server/index.ts",
    "migrate": "prisma migrate dev",
    "seed": "prisma db seed",
    "test": "vitest",
    "build:renderer": "vite build",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "class-variance-authority": "^0.7.0",
    "express": "^4.19.2",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "electron": "^31.3.1",
    "postcss": "^8.4.41",
    "prisma": "^5.18.0",
    "tailwind-animate": "^1.0.7",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6",
    "vitest": "^2.1.1",
    "wait-on": "^8.0.2"
